  <SyntaxDefinition name="MarkDown" extensions=".md">
	  <Color name="Heading"        foreground="#FF00C0FF" exampleText="# Title #" />
	  <Color name="Emphasis"       fontStyle="italic" exampleText="*this* is important!" />
